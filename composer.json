{
    "name": "ruudk/readme-examples-sync-hook",
    "description": "Automatically sync PHP code examples with readme file",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "dev"
    ],
    "authors": [
        {
            "name": "Ruud Kamphuis",
            "email": "ruudk@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "captainhook/captainhook": "^5.25",
        "ergebnis/composer-normalize": "^2.47",
        "erickskrauch/php-cs-fixer-custom-fixers": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.85",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "ticketswap/phpstan-error-formatter": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Ruudk\\ReadmeExamplesSyncHook\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        }
    }
}
